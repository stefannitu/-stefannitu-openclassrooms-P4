}

.navbar-toggle .icon-bar {
    background-color: rgba(0, 0, 0, .5);
    width: 26px
}

.nav>li>.dropdown a {
    background: 0 0 !important;
    display: block;
    padding: 14px 15px
}

.mg-md {
